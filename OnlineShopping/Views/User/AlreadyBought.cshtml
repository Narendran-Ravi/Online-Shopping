@model IEnumerable<OnlineShopping.DomainModel.CompletedOrders>
@{
    ViewBag.Title = "Already Bought";

}


<table class="table table-hover">
    <tr>
        <th>Request Id</th>

        <th>Product Id</th>

        <th>Product Name</th>
        <th>Price</th>

        <th></th>



    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.RequestId</td>

            <td>@item.ProductId</td>
            <td>@item.producttable.ProductName</td>
            <td>@item.producttable.Price</td>
           


        </tr>
    }
</table>
